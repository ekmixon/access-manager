{
  "Logging": {
    "IncludeScopes": {},
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "System.Net": "Warning" 
    },
    "EventLog": {
      "LogLevel": {
        "Default": "Error",
        "Lithnet": "Information"
      }
    }
  },
  "Agent": {
    "Enabled": true,
    "Interval": 60,
    "AdvancedAgent": {
      "Enabled": true,
      "Server": "localhost",
      "AuthenticationMode": "Aadj" // Aadj, Ssa ,
    },

    "PasswordManagement": {
      "Enabled": true,
      "PasswordPolicy": {
        "PasswordLength": 24,
        "PasswordCharacters": null,
        "UseUpper": true,
        "UseLower": true,
        "UseSymbol": true,
        "UseNumeric": true,
        "MaximumPasswordAgeDays": 1
      },
      "ActiveDirectorySettings": {
        "LithnetLocalAdminPasswordAttributeBehaviour": "Populate",
        "MsMcsAdmPwdAttributeBehaviour": "Populate",
        "LithnetLocalAdminPasswordHistoryDaysToKeep": 30
      }
    }
  }
}